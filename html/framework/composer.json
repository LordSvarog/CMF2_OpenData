{
    "name": "yii2-developer/yii2-cmf2",
    "description": "The CMF2 is based on Yii2",
    "keywords": [
        "yii2",
        "framework",
        "basic",
        "project template"
    ],
    "type": "project",
    "license": "BSD-3-Clause",
    "authors": [
        {
            "name": "krok",
            "email": "yii2.developer@gmail.com"
        }
    ],
    "minimum-stability": "stable",
    "repositories": [
        {
            "type": "composer",
            "url": "https://asset-packagist.org"
        },
        {
            "type": "composer",
            "url": "https://packagist.dev-vps.ru"
        },
        {
            "type": "path",
            "url": "extensions/*/"
        }
    ],
    "require": {
        "php": ">=7.1.0",
        "yii2-developer/env": "~0.0.1",
        "yii2-developer/yii2-access": "~1.0.0",
        "yii2-developer/yii2-application": "~0.0.1",
        "yii2-developer/yii2-auth": "~2.0.0",
        "yii2-developer/yii2-backup": "~1.2.0",
        "yii2-developer/yii2-bootbox": "~0.0.1",
        "yii2-developer/yii2-catch-all": "~1.2.0",
        "yii2-developer/yii2-configure": "~1.2.0",
        "yii2-developer/yii2-content": "~2.0.0",
        "yii2-developer/yii2-datetime-formatter": "~0.0.1",
        "yii2-developer/yii2-editor": "~2.0.0",
        "yii2-developer/yii2-extend": "~1.0.0",
        "yii2-developer/yii2-filesystem": "~1.0.0",
        "yii2-developer/yii2-glide": "~2.1.0",
        "yii2-developer/yii2-language": "~1.1.0",
        "yii2-developer/yii2-mailer": "~1.2.0",
        "yii2-developer/yii2-maxlength": "~0.0.1",
        "yii2-developer/yii2-paper-dashboard": "~1.2.0",
        "yii2-developer/yii2-queue": "~1.0.0",
        "yii2-developer/yii2-robots": "~1.2.0",
        "yii2-developer/yii2-select2": "~0.0.1",
        "yii2-developer/yii2-sentry": "~1.1.0",
        "yii2-developer/yii2-sms": "~0.0.1",
        "yii2-developer/yii2-system": "~1.0.0",
        "yii2-developer/yii2-tinymce": "~2.0.0",
        "yii2-developer/yii2-tinymce-uploader": "~1.0.0",
        "yii2-developer/yii2-transliterate": "~0.0.1",
        "yiisoft/yii2": "~2.0.15",
        "yiisoft/yii2-bootstrap": "~2.0.0",
        "yiisoft/yii2-imagine": "~2.0.0",
        "yiisoft/yii2-jui": "~2.0.0",
        "yiisoft/yii2-redis": "~2.0.0"
    },
    "require-dev": {
        "codeception/base": "^2.2.3",
        "codeception/verify": "~0.3.1",
        "guzzlehttp/guzzle": ">=4.1.4 <7.0",
        "yiisoft/yii2-debug": "~2.0.0",
        "yiisoft/yii2-gii": "~2.0.0"
    },
    "config": {
        "optimize-autoloader": true,
        "process-timeout": 1800,
        "sort-packages": true
    },
    "scripts": {
        "post-autoload-dump": "curl --silent --header \"Accept: text/plain\" https://security.symfony.com/check_lock --form lock=@composer.lock"
    }
}
